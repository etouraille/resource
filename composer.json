{
    "repositories": [{
        "type": "package",
        "package": {
            "name": "etouraille/simpleMySQL",
            "version": "master",
            "source": {
                "url": "https://github.com/etouraille/simpleMySQL.git",
                "type": "git",
                "reference": "master"
            }
        }
    }],
    "require":
    {
        "silex/silex":"~1.2",
        "curl/curl":"dev-master",
        "etouraille/simpleMySQL": "dev-master",
        "respect/validation" : "dev-master"
    },
    "autoload": {
        "psr-4": {
            "OP\\": "src/",
            "simpleMySQL\\": "vendor/etouraille/simpleMySQL/src"
        }
    }
}